{
    "name": "meabed/tailwind-preset",
    "description": "A Tailwind CSS preset for Laravel.",
    "keywords": [
        "laravel",
        "preset",
        "tailwind",
        "tailwindcss"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Zak Nesler",
            "email": "zak@nesler.dev"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3 | 8.*",
        "laravel/ui": "^2.5",
        "illuminate/console": "^7|^8",
        "illuminate/filesystem": "^7|^8",
        "illuminate/support": "^7|^8"
    },
    "autoload": {
        "psr-4": {
            "Meabed\\TailwindPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meabed\\TailwindPreset\\TailwindServiceProvider"
            ]
        }
    }
}
